{
  // Enforcing options
  "bitwise": false,
  "camelcase": false,
  "curly": true,
  "eqeqeq": true,
  "forin": false,
  "immed": false,
  //"indent": 2,
  "latedef": true,
  "newcap": false,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "plusplus": false,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "strict": true,
  "trailing": true,
  //"maxparam": 3,
  //"maxdepth": 2,
  //"maxstatements": 4,
  "maxcomplexity": 10,
  //"maxlen": 120,

  // Relaxing options
  "asi": false,
  "boss": true,
  "debug": true,
  "eqnull": false,
  "esnext": false,
  "evil": true, // Because the recompiler generates new functions!
  "expr": true,
  "funcscope": true,
  "globalstrict": true,
  "iterator": false,
  "lastsemic": false,
  "laxbreak": false,
  "laxcomma": false,
  "loopfunc": true,
  "multistr": false,
  "proto": false,
  "scripturl": true,
  "smarttabs": false,
  "shadow": false,
  "sub": true,
  "supernew": true,
  "validthis": true,

  // Environments
  "browser": true,
  "devel": true,
  "jquery": true,

  "globals": {
    "JSSMS": false,
    // Globals defined in src/utils.js.
    "DEBUG": false,
    "ENABLE_DEBUGGER": false,
    "ENABLE_COMPILER": false,
    "WRITE_MODE": false,
    "READ_MODE": false,
    "ENABLE_SERVER_LOGGER": false,
    "SYNC_MODE": false,
    "ACCURATE": false,
    "LITTLE_ENDIAN": false,
    "FORCE_TYPED_ARRAYS": false,
    "SUPPORT_TYPED_ARRAYS": false,
    "FORCE_DATAVIEW": false,
    "SUPPORT_DATAVIEW": false,
    "FORCE_DESTRUCTURING": false,
    "SUPPORT_DESTRUCTURING": false,
    "SAMPLE_RATE": false,
    "DEBUG_TIMING": false,
    "REFRESH_EMULATION": false,
    "ACCURATE_INTERRUPT_EMULATION": false,
    "LIGHTGUN": false,
    "VDP_SPRITE_COLLISIONS": false,
    "PAGE_SIZE": false,
    // Globals defined in src/vdp.js.
    "NTSC": false,
    "PAL": false,
    "SMS_X_PIXELS": false,
    "SMS_Y_PIXELS_NTSC": false,
    "SMS_Y_PIXELS_PAL": false,
    "SMS_WIDTH": false,
    "SMS_HEIGHT": false,
    "GG_WIDTH": false,
    "GG_HEIGHT": false,
    // Globals defined in src/keyboard.js.
    "KEY_UP": false,
    "KEY_DOWN": false,
    "KEY_LEFT": false,
    "KEY_RIGHT": false,
    "KEY_FIRE1": false,
    "KEY_FIRE2": false,
    "KEY_START": false,
    // Globals defined in src/z80.js.
    "HALT_SPEEDUP": false,
    "OP_STATES": false,
    "OP_CB_STATES": false,
    "OP_DD_STATES": false,
    "OP_INDEX_CB_STATES": false,
    "OP_ED_STATES": false,
    // Globals defined in src/compiler.
    "Recompiler": false,
    "SyncWriter": false,
    "SyncReader": false,
    "opcodeTable": false
  }
}
